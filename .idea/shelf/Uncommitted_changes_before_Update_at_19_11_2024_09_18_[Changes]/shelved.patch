+package org.hbrs.se1.ws24.exercises.uebung4.UserStory;
+
+import org.hbrs.se1.ws24.exercises.uebung2.member.Member;
+
+import java.io.Serializable;
+
+public class UserStory implements Member, Serializable {
+
+    private Integer id;
+    private String title;
+    private String akzeptanzKriterium;
+    private String projekt;
+    private Integer risiko;
+    private Integer aufwand;
+    private Integer nutzen;
+    private Integer mehrwert;
+    private double prioritaet;
+
+    public UserStory(Integer id,
+                     String title,
+                     String akzeptanzKriterium,
+                     String projekt,
+                     Integer risiko,
+                     Integer aufwand,
+                     Integer nutzen,
+                     Integer mehrwert) {
+        this.id = id;
+        this.title = title;
+        this.akzeptanzKriterium = akzeptanzKriterium;
+        this.projekt = projekt;
+        if ((risiko < 1 || risiko > 5) ||
+                (aufwand < 1 || aufwand > 5) ||
+                (nutzen < 1 || nutzen > 5) ||
+                (mehrwert < 1 || mehrwert > 5)
+            ) {
+            throw new IllegalArgumentException("Kein gültiger Wert für Risiko, Aufwand, Nutzen oder Mehrwert angeben.\n" +
+                    "Die Werte müssen größer 0 und kleiner 6 sein!");
+        } else {
+            this.risiko = risiko;
+            this.aufwand = aufwand;
+            this.nutzen = nutzen;
+            this.mehrwert = mehrwert;
+            this.prioritaet = berechnePrio();
+        }
+
+
+    }
+
+    private double berechnePrio() {
+        return (double) (risiko + aufwand) / (nutzen + mehrwert);
+    }
+
+    @Override
+    public Integer getID() {
+        return id;
+    }
+
+    public String getTitle() {
+        return title;
+    }
+
+    public String getAkzeptanzKriterium() {
+        return akzeptanzKriterium;
+    }
+
+    public String getProjekt() {
+        return projekt;
+    }
+
+    public Integer getRisiko() {
+        return risiko;
+    }
+
+    public Integer getAufwand() {
+        return aufwand;
+    }
+
+    public Integer getNutzen() {
+        return nutzen;
+    }
+
+    public Integer getMehrwert() {
+        return mehrwert;
+    }
+
+    public double getPrioritaet() {
+        return prioritaet;
+    }
+
+    public String toString() {
+        return "User Story ID: " + getID() + "\n" +
+                "Titel: " + getTitle() + "\n" +
+                "Akzeptanzkriterium: " + getAkzeptanzKriterium() + "\n" +
+                "Risiko: " + getRisiko() + "\n" +
+                "Aufwand: " + getAufwand() + "\n" +
+                "Nutzen: " + getNutzen() + "\n" +
+                "Mehrwert: " + getMehrwert() + "\n" +
+                "Projekt: " + getProjekt() + "\n" +
+                "Priorität: " + getPrioritaet() + "\n";
+    }
+}
